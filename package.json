{
  "name": "synth-cdk-to-cloudformation",
  "version": "0.1.0",
  "bin": {
    "synth-cdk-to-cloudformation": "bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "tsc && npx cdk synth",
    "synth:product": "tsc && npx cdk --app 'npx ts-node --prefer-ts-exts bin/product.ts' --path-metadata false --version-reporting false synth --quiet"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.122.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "aws-cdk": "1.122.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-s3": "^1.122.0",
    "@aws-cdk/core": "1.122.0",
    "source-map-support": "^0.5.16",
    "yaml": "^1.10.2"
  }
}
